local plr = game.Players.LocalPlayer
local GroupId = 10914980

if plr:IsInGroup(GroupId) then
	players = {}
for _,p in pairs(game.Players:GetPlayers()) do
	table.insert(players, p.Name)
	table.sort(players)
end

local abbreviations = {'','K','M','B','T','Qd','Qn','Sx','Sp','O','N','D','Und','DD','Td','Qtd','QnD' ,'SxD' ,'SpD' ,'OcD' ,'NoD' ,'VgN' ,'UvG' ,'DvG' ,'TvG' ,'QtV'}

local function roundNumber(Amount)
	local Text = tostring(Amount)
	local chosenAbbreviation

	for i = 1, #abbreviations do
		if tonumber(Amount) < 10^(i*3) then
			Text = math.floor(Amount/((10^((i-1)*3))/100))/(100).. abbreviations[i]
			break
		end
	end
	return Text
end

--// Meta Tables
--// Speed
local mt = getrawmetatable(game)

setreadonly(mt, false)
local old = mt.__index

mt.__index = function(o, k)
 if tostring(o) == "Humanoid" and tostring(k) == "WalkSpeed" or tostring(o) == "Humanoid" and tostring(k) == "JumpPower" then
   print("okokokok")
     return 16 
   end

 return old(o, k)

end

local VirtualUser=game:service'VirtualUser'
game:service'Players'.LocalPlayer.Idled:connect(function()
	VirtualUser:CaptureController()
	VirtualUser:ClickButton2(Vector2.new())
end)

local SolarisLib = loadstring(game:HttpGet("https://solarishub.dev/SolarisLib.lua"))()

local win = SolarisLib:New({
	Name = "Cloudy Hub",
	FolderToSave = "CloudyHubFiles"
})

local tab = win:Tab("Farming")
local tab2 = win:Tab("Dropping")
local tab3 = win:Tab("Looping")
local tab4 = win:Tab("Misc")
local tab5 = win:Tab("Info")
local tab6 = win:Tab("Settings")

local Section1 = tab:Section("Farming")
local Section2 = tab:Section("Extra")
local Section3 = tab2:Section("Dropping")
local Section4 = tab4:Section("Misc")
local Section5 = tab5:Section("Player Data")
local Section6 = tab5:Section("Information")
local Section7 = tab6:Section("Settings")
local Section8 = tab3:Section("Looping")
local Section9 = tab4:Section("Strength Spy")
local Section10 = tab6:Section("Player Settings")
local Section11 = tab5:Section("Developers")
local Section12 = tab4:Section("Premium Misc")
local Section13 = tab3:Section("Premium Looping")


getgenv().waittime = 0.7
getgenv().tool = "Double Weight"
getgenv().Mag = 5

AutoLifing = false
LockPlayer = false
FollowPlayer = false
EquipTool = false
RainbowGloves = false
LoopPlayer = false
AutoKilling = false

game:GetService('RunService').Stepped:connect(function()
	user = getgenv().player
	if FollowPlayer then
		plr.Character.HumanoidRootPart.CFrame = game:GetService("Players")[user].Character.HumanoidRootPart.CFrame + game:GetService("Players")[user].Character.HumanoidRootPart.CFrame.lookVector * -5
		wait(.2)
	end
end)

function Farm(value)
    if value == true then
        Debounce = getgenv().waittime
        AutoLifing = true
        repeat
            for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                if v.Name == getgenv().tool then
                    v:Activate()
                else
                    for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                        if v.Name == getgenv().tool then
                            v:Activate()
                            v.Parent = game.Players.LocalPlayer.Character
                        end
                    end
                end
            end
            wait(Debounce)
        until AutoLifing == false
    elseif value == false then
        AutoLifing = false
    end
end

function Lock(value)
    if value == true then
        LockPlayer = true
        repeat
            for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                if v:IsA("Part") then
                    v.Anchored = true
                end
            end
            for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                if v:IsA("MeshPart") then
                    v.Anchored = true
                end
            end
            wait(1)
        until LockPlayer == false
    elseif value == false then
        LockPlayer = false
        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("Part") then
				v.Anchored = false
			end
		end
        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
			if v:IsA("MeshPart") then
				v.Anchored = false
			end
		end
    end
end

function Chase(value)
    user = getgenv().player
    if value == true then
        if game.Players[user] then
            FollowPlayer = true
        end
    elseif value == false then
        FollowPlayer = false
    end
end

function Use(value)
    if value == true then
        EquipTool = true
        repeat
            for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                if v.Name == getgenv().tool then
                    v:Activate()
                    v.Parent = game.Players.LocalPlayer.Character
                end
            end
            wait(1)
        until EquipTool == false
    elseif value == false then
        EquipTool = false
    end
end

function Switching(value)
    if value == true then
        RainbowGloves = true
        repeat
            game.ReplicatedStorage.Remotes.SellWep:FireServer("Wamen")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Waffle")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Donut")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Pizza")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Snow Camo")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Dirt")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Red")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Green")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Desert Camo")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Bape")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Bape Blue")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Blue")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Spongebob")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Wanwood")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Adurite")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Rainbow")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Cartoon")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Grass")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Black Iron")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Pink")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Bape Pink")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Doge")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Trump")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("White")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Gold")
			wait(.1)
			game.ReplicatedStorage.Remotes.SellWep:FireServer("Bluesteel")
            wait(.1)
        until RainbowGloves == false
    elseif value == false then
        RainbowGloves = false
    end
end

function Kill(value)
    Target = getgenv().User
    if value == true then
        if game.Players[Target] then
            LoopPlayer = true
            local field = Target

			local LocalUser = game.Players.LocalPlayer
			local ENEMY = field
            repeat
                game.ReplicatedStorage.Remotes.Human_Punch:FireServer(LocalUser.Character.RightHand, "RightPunch", workspace[ENEMY].UpperTorso,5,true,"RightPunch",game.Players.LocalPlayer.Backpack.Boxing.Handle.Hit,5)
				game.ReplicatedStorage.Remotes.Human_Punch:FireServer(plr.Character.LeftHand, "LeftPunch", workspace[ENEMY].UpperTorso,5,true,"LeftPunch",game.Players.LocalPlayer.Backpack.Boxing.Handle.Hit,5)
				game.ReplicatedStorage.Remotes.Human_Punch:FireServer(LocalUser.Character.RightHand, "RightPunch", workspace[ENEMY].UpperTorso,5,true,"RightPunch",game.Players.LocalPlayer.Backpack.Boxing.Handle.Hit,5)
                wait(.01)
            until LoopPlayer == false
        end
    elseif value == false then
        LoopPlayer = false
    end
end

function AutoKill(value)
	if value == true then
		AutoKilling = true
		repeat
			local plr = game.Players.LocalPlayer
			for i,v in pairs(game.Players:GetPlayers()) do
    			if v.Name ~= plr.Name then
        			Enemy = v.Character.HumanoidRootPart
        			User = plr.Character.HumanoidRootPart
        			print(Enemy.Parent)
        			print(Enemy.Name)
        			if (Enemy.Position - User.Position).magnitude <= getgenv().Mag then
            			game.ReplicatedStorage.Remotes.Human_Punch:FireServer(plr.Character.RightHand, "RightPunch", workspace[v.Name].UpperTorso,5,true,"RightPunch",game.Players.LocalPlayer.Backpack.Boxing.Handle.Hit,5)
        			end
    			end
			end
			wait(.02)
		until AutoKilling == false
	elseif value == false then
		AutoKilling = false
	end
end

local Lift = Section1:Toggle("Auto Farm", false,"Toggle", function(f)
	Farm(f)
end)

local Locking = Section2:Toggle("Lock Player", false,"Toggle", function(l)
    Lock(l)
end)

local Ping = Section2:Button("Lower Ping", function()
	local decalsyeeted = true -- Leaving this on makes games look shitty but the fps goes up by at least 20.
    local g = game
	local w = g.Workspace
	local l = g.Lighting
	local t = w.Terrain
	t.WaterWaveSize = 0
	t.WaterWaveSpeed = 0
	t.WaterReflectance = 0
	t.WaterTransparency = 0
	l.GlobalShadows = false
	l.FogEnd = 9e9
	l.Brightness = 0
	settings().Rendering.QualityLevel = "Level01"
	for i,v in pairs(g:GetDescendants()) do
		if v:IsA("Part") or v:IsA("Union") or v:IsA("MeshPart") or v:IsA("CornerWedgePart") or v:IsA("TrussPart") then
			v.Material = "Plastic"
			v.Reflectance = 0
		elseif v:IsA("Decal") and decalsyeeted then
			v.Transparency = 1
		elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
			v.Lifetime = NumberRange.new(0)
		elseif v:IsA("Explosion") then
			v.BlastPressure = 1
			v.BlastRadius = 1
		end
	end
	for i,e in pairs(l:GetChildren()) do
		if e:IsA("BlurEffect") or e:IsA("SunRaysEffect") or e:IsA("ColorCorrectionEffect") or e:IsA("BloomEffect") or e:IsA("DepthOfFieldEffect") then
			e.Enabled = false
		end
	end
end)

local UI = Section2:Button("Remove UI", function()
    game:GetService("Players").LocalPlayer.PlayerGui.HUD:Destroy()
	game:GetService("ReplicatedFirst").TourneyQ:Destroy()
end)

local Cam = Section2:Button("Remove Camera", function()
    game:GetService("CoreGui").RobloxGui.Backpack:Destroy()
	game:GetService("Workspace").Camera:Destroy()
end)

local Safe = Section2:Button("Tp to Safe Zone", function()
    local char = plr.Character
	char.HumanoidRootPart.CFrame = CFrame.new(-1977.9436, 510, -5295.2124, 1, 0, 0, 0, 1, 0, 0, 0, 1)
end)

