local str = game.ReplicatedStorage.Data[plr.Name].Strength;
str:GetPropertyChangedSignal("Value"):Connect(function()
	TotalStrength:Set("Strength: "..roundNumber(str.Value));
end)

local str = game:GetService("Players").LocalPlayer.leaderstats.Strength;
local curvalue = str.Value;
local earned = 0;
str:GetPropertyChangedSignal("Value"):Connect(function()
	local delta = (str.Value - curvalue);
	earned += delta;
	StrengthGained:Set("Strength Gained: "..roundNumber(earned));
	curvalue = str.Value;
end)

local str = game:GetService("Players").LocalPlayer.leaderstats.Wins;
local curvalue = str.Value;
local earned = 0;
str:GetPropertyChangedSignal("Value"):Connect(function()
	local delta = (str.Value - curvalue);
	WinsGained:Set("Wins Gained: "..roundNumber(earned));
	curvalue = str.Value;
end)

