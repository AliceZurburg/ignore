local CountTools = Section6:Button("Count Tools", function()
    local num = 0
	for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
		if v.Name == getgenv().tool then
			num += 1
		end
	end
    local endnum = num
    CurrentWeights:Set("Amount Of Tools: "..endnum)
end)

