local Small = Section4:Button("Small", function()
    for i,v in pairs(game.Players.LocalPlayer.Character.Humanoid:GetChildren()) do
		if v:IsA("NumberValue") then
			v:Destroy()
		end
	end
end)

local GiveGloves = Section4:Button("Give All Gloves", function()
    for i,v in pairs(game:GetService("ReplicatedStorage").BoxingSkins:GetChildren()) do
        local args = {
            [1] = ""..v.Name,
        }
        
        game:GetService("ReplicatedStorage").Remotes["SetMagic1"]:FireServer(unpack(args))
    end
end)

local BlueFog = Section4:Button("Clear Fog", function()
    game:GetService("Lighting").FogStart = 0
    game:GetService("Lighting").FogEnd = 9999999999999
    game:GetService("Lighting").Atmosphere:Destroy()
end)

local SpecialSmall = Section12:Button("Special Small", function()
