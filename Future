	game.Players.LocalPlayer.Character.Humanoid.BodyDepthScale:Destroy()
	game.Players.LocalPlayer.Character.Humanoid.BodyHeightScale:Destroy()
	game.Players.LocalPlayer.Character.Humanoid.BodyWidthScale:Destroy()
end)

local TotalStrength = Section5:Label("Strength : 0")
local StrengthGained = Section5:Label("Strength Gained: 0")
local WinsGained = Section5:Label("Wins Gained: 0")
local CurrentWeights = Section6:Label("Amount Of Tools: 0")

local CountTools = Section6:Button("Count Tools", function()
    local num = 0
	for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
		if v.Name == getgenv().tool then
			num += 1
		end
	end
    local endnum = num
    CurrentWeights:Set("Amount Of Tools: "..endnum)
end)

