    setclipboard("discord.gg/XVJvy3T7rq")
end)

local CurrentTool = Section7:Label(""..getgenv().tool)

local SwitchTool = Section7:Button("Switch Tool", function()
    if getgenv().tool == "Double Weight" then
        getgenv().tool = "Dumbell"
        CurrentTool:Set(""..getgenv().tool)
    elseif getgenv().tool == "Dumbell" then
        getgenv().tool = "Double Weight"
        CurrentTool:Set(""..getgenv().tool)
    end
end)

local ChosenTime = Section7:Slider("Wait Time", 0,2,0.7,0.1,"Slider", function(t)
    getgenv().waittime = t
end)

local WalkspeedChanger = Section10:Slider("Speed", 16,200,25,0.1,"Slider", function(speed)
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = speed
end)

local JumppowerChanger = Section10:Slider("Jump", 50,200,50,0.1,"Slider", function(jump)
	game.Players.LocalPlayer.Character.Humanoid.JumpPower = jump
end)

local str = game.ReplicatedStorage.Data[plr.Name].Strength;
str:GetPropertyChangedSignal("Value"):Connect(function()
	TotalStrength:Set("Strength: "..roundNumber(str.Value));
end)

local str = game:GetService("Players").LocalPlayer.leaderstats.Strength;
local curvalue = str.Value;
local earned = 0;
str:GetPropertyChangedSignal("Value"):Connect(function()
	local delta = (str.Value - curvalue);
	earned += delta;
	StrengthGained:Set("Strength Gained: "..roundNumber(earned));
	curvalue = str.Value;
end)

local str = game:GetService("Players").LocalPlayer.leaderstats.Wins;
local curvalue = str.Value;
local earned = 0;
str:GetPropertyChangedSignal("Value"):Connect(function()
	local delta = (str.Value - curvalue);
	WinsGained:Set("Wins Gained: "..roundNumber(earned));
	curvalue = str.Value;
end)

